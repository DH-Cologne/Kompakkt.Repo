{
  "name": "kompakkt.repo",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "lint": "eslint -c .eslintrc.js ./src/**/*.ts",
    "prettier": "prettier --config .prettierrc.js 'src/**/*.{ts,html,scss}' --write",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "e2e": "concurrently \"ng serve --port 1503\" \"wait-on http-get://localhost:1503 && npm run cy:run\" --kill-others --success first",
    "e2e-gui": "concurrently \"ng serve --port 1503\" \"wait-on http-get://localhost:1503 && npm run cy:open\" --kill-others --success first"
  },
  "contributors": [
    {
      "name": "Jan G. Wieners",
      "email": "jan.wieners@uni-koeln.de",
      "url": "https://jan-wieners.de"
    },
    {
      "name": "Kai Niebes",
      "email": "kai.niebes@outlook.com",
      "url": "https://github.com/HeyItsBATMAN"
    },
    {
      "name": "Zoe Schubert"
    }
  ],
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier"
    }
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "quoteProps": "as-needed",
    "trailingComma": "all",
    "bracketSpacing": true,
    "arrowParens": "avoid",
    "parser": "typescript",
    "requirePragma": false,
    "insertPragma": false,
    "endOfLine": "lf",
    "printWidth": 80
  },
  "dependencies": {
    "@angular/animations": "^10.1.3",
    "@angular/cdk": "^10.2.2",
    "@angular/common": "~10.1.3",
    "@angular/compiler": "~10.1.3",
    "@angular/core": "~10.1.3",
    "@angular/flex-layout": "^10.0.0-beta.32",
    "@angular/forms": "~10.1.3",
    "@angular/material": "^10.2.2",
    "@angular/platform-browser": "~10.1.3",
    "@angular/platform-browser-dynamic": "~10.1.3",
    "@angular/router": "~10.1.3",
    "@kompakkt/shared": "git+https://github.com/DH-Cologne/Kompakkt.Shared.git",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "fscreen": "^1.0.2",
    "particles.js": "^2.0.0",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1001.3",
    "@angular/cli": "~10.1.3",
    "@angular/compiler-cli": "~10.1.3",
    "@angular/language-service": "~10.1.3",
    "@types/fscreen": "^1.0.1",
    "@types/node": "^14.0.14",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "concurrently": "^5.3.0",
    "cypress": "^5.0.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "prettier": "^2.0.5",
    "ts-node": "~8.10.1",
    "typescript": "~3.9.6",
    "wait-on": "^5.2.0"
  }
}
